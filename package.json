{
  "name": "bot-messenger-template",
  "version": "1.0.0",
  "description": "Bot Messenger Platform",
  "main": "bot.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build && mkdir build/tmp",
    "dev": "NODE_PATH=. nodemon server/bot.js --exec babel-node --presets es2015",
    "build-server": "babel -d ./build ./server -s",
    "build": "npm run clean  && npm run build-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && node ./build/bot.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
