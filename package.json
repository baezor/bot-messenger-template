  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.7.17",
    "config": "^1.20.4",
    "curlrequest": "^1.0.1",
    "ejs": "^2.4.2",
    "express": "^4.13.4",
    "googlemaps": "^1.12.0",
    "mongoose": "^4.7.6",
    "mongoose-class-wrapper": "^0.8.1",
    "mongoose-unique-validator": "^1.0.3",
    "mongoose-validator": "^1.2.5",
    "q": "~1.4.1",
    "request": "^2.72.0",
    "slugify": "^1.0.2",
    "babel-cli": "~6.8.0",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.6.0",
    "body-parser": "^1.15.0"
  },
